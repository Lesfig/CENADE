$("#wrapper").toggleClass("toggled");

$("#principal-content").html('<%= j render 'form', consulta: @consulta, paciente: @paciente, from_ficha: true, formats: 'html' %>');

$('.select-paciente').val("<%=j params[:paciente].to_s %>").trigger("change");
$('.select-paciente').attr("disabled","true");
$('#consulta_paciente_id').val("<%=j params[:paciente].to_s %>");

$('.select-area').val("<%=j params[:area].to_s %>").trigger("change");
$('.select-area').attr("disabled","true");
$('#consulta_area_id').val("<%=j params[:area].to_s %>");

$('.select-doctor').val("<%=j params[:profesional].to_s %>").trigger("change");

$('.select2').attr("style","width: 100%;");//- Sin esto los select se ven ajustados



