
<table id="table-reportes" class= "table table-condensed">
  <thead>
    <tr class="active">

      <th>Profesional de la Salud</th>
      <th>Cantidad Atentidos</th>
      <th>Area</th>
    </tr>
  </thead>
  <% if turnos.blank? %>
    <tfoot>
      <tr>
        <th colspan="8" scope="col">No se encontraron coincidencias </th>
      </tr>
    </tfoot>
   <% end %>
  <tbody>
    <% turnos.each do |turno| %>
    <tr>

      <td ><%=turno.doctor.persona_full_name %> </td>
      <td ><%=turno.obtener_cantidad%></td>
      <td align="right"><%=turno.area_nombre%></td>


    </tr>
    <% end %>
  </tbody>
</table>

<script type="text/javascript">

      reportesUI.initScript();
      <% if !flash.notice.blank? %>
        var n = noty({text: '<%=notice%>', type: 'success'});
      <% end %>
      <% if !flash.alert.blank?%>
        var n = noty({text: '<%=alert%>', type: 'error'});
      <% end %>

</script>