		<div class="data"> <%=@consulta.laboratorios%> </div>
	</div>
</div></br>

<div class="row">
	<div class="form-group col-md-12">
		<label class="control-label">Otros exámenes</label>
		<div class="data"> <%=@consulta.otros_examenes%> </div>
	</div>
</div></br>

<div class="row">
	<div class="form-group col-md-12">
		<label class="control-label">Tratamiento</label>
		<div class="data-bordered"> <%=@consulta.tratamiento%> </div>
	</div>
</div>