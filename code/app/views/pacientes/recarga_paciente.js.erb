
$('#modal-lg').modal('hide');
$('.paciente_select').html("<%= escape_javascript(options_for_select(Hash[Paciente.all.map{|a| [a.persona_full_name, a.id]}], selected: @paciente.id).insert(0, 'Seleccione un paciente')) %>");
$('.paciente_select').select2('destroy');
$('.paciente_select').select2({
  data: [{ id: "<%=j @paciente.id.to_s %>", text: "<%=@paciente.persona_full_name %> " }]
});
$('.paciente_select').val("<%=j @paciente.id.to_s %>").trigger("change");
$('.paciente_data').html("<%= j render params[:root].to_s()+'/paciente_form' %>");

<% if params[:encargado] %>
	<%if !@paciente.encargado.nil? %>
		$('.encargado_data').html("<%= j render params[:root].to_s()+'/encargado_form' %>");
	<%end%>
<%end%>
