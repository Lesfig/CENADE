<% flash.each do |key, value| %>
<% if key != "timedout" %>
  <div class="col-md-4">
  <div class="alert alert-<%= key %>">
    <a href="#" data-dismiss="alert" class="close"><span class="glyphicon glyphicon-remove icon-large"></span></a>
    <ul>
        <%= value %>
      
    </ul>
  </div>
  </div>
  
<% end %>
<% end %>